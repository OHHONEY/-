.border {
  height: .5px;
  margin: 0 40rpx;
  align-self: stretch;
  background: linear-gradient(to left, #EB6592, #F6C050);
}

navigator {
  align-self: stretch;
  display: flex;
  height: 140rpx;
  margin: 0 40rpx;
  box-sizing: border-box;
}

.page_setting {
  margin: 80rpx 40rpx 0;
  height: 100rpx;
}

navigator:last-of-type {
  height: 100rpx;
}

navigator view {
  font-size: 34rpx;
  color: #382C12;
  display: flex;
  align-items: center;
  justify-content: start;
}

navigator view::before{
  content: '';
  display: block;
  width: 56rpx;
  height: 56rpx;
  margin: 0 20rpx 0 0;
  background-position: center;
  background-repeat: no-repeat;
  background-size: 100% auto;
}

.page_friendship::before {
  background-image: url('data:image/png;base64,')
}

.page_impressive::before {
  background-image: url('data:image/png;base64,')
}

.page_my-life::before {
  background-image: url('data:image/png;base64,')
}

.page_reallyOwn::before {
  background-image: url('data:image/png;base64,')
}

.page_setting view::before {
  background-image: url('data:image/png;base64,')
}

.page_sign-out::before {
  background-image: url('data:image/png;base64,')
}

.userinfo {
  height: 140rpx;
  width: 100%;
  background: #fff;
  border-right: none;
  display: flex;
  flex-direction: row;
  align-items: center;
  transition: all 300ms ease;
  margin: 60rpx 40rpx 80rpx;
}

.userinfo-avatar {
  width: 180rpx;
  height: 180rpx;
  margin: 0;
  border-radius: 50%;
  background-size: cover;
  background-color: white;
}

.userinfo text {
  font-size: 32rpx;
  color: #2A210D;
}

.userinfo-avatar:after {
  border: none;
}
